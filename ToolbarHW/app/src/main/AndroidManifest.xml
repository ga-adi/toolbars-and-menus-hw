<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adi.ho.jackie.toolbarhw">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PocketcastActivity"
            android:theme="@style/PocketcastTheme" />
        <activity
            android:name=".FacetuneActivity"
            android:theme="@style/FacetuneTheme" />
        <activity android:name=".TedActivity"
            android:theme="@style/TedTheme"></activity>
    </application>

</manifest>
