<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TedTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:background">@color/colorPrimaryTed</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkTed</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColor">#000000</item>

    </style>
    <style name="CustomTabLayout" parent="Widget.Design.TabLayout">
        <item name="tabSelectedTextColor">@color/colorAccent</item>
        <item name="tabBackground">@color/colorPrimaryFacetune</item>
    </style>
</resources>